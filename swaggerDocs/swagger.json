      "/generateToken": {
        "post": {
          "summary": "Generate a JWT token",
          "description": "Generates a JWT token for the authenticated user with a 24-hour expiration.",
          "tags": ["Authentication"],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "owner": {
                      "type": "string",
                      "example": "shavy"
                    },
                    "token": {
                      "type": "string",
                      "example": "rag"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Token successfully generated.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "token": {
                        "type": "string",
                        "description": "The generated JWT token."
                      }
                    }
                  },
                  "examples": {
                    "success": {
                      "summary": "Successful token generation",
                      "value": {
                        "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJvd25lciI6InVzZXJfaWQiLCJpYXQiOjE2NDc1OTY4MDB9"
                      }
                    }
                  }
                }
              }
            },
            "401": {
              "description": "Unauthorized access. Authentication is required."
            }
          }
        }
      },
